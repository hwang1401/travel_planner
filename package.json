{
  "name": "travel-planner",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build:tokens": "node scripts/build-tokens.cjs",
    "dev": "npm run build:tokens && vite",
    "build": "npm run build:tokens && vite build",
    "preview": "vite preview",
    "collect-timetable": "node scripts/collect-timetable/runner.js",
    "collect-timetable:list": "node scripts/collect-timetable/runner.js --list",
    "collect-timetable:dry": "node scripts/collect-timetable/runner.js --dry-run",
    "gtfs-parse": "node scripts/collect-timetable/gtfs-parser.js",
    "rag-seed": "node scripts/rag-seed.js",
    "rag-seed:full": "node scripts/rag-seed.js --region osaka --type food",
    "rag-seed:all": "node scripts/rag-seed.js --all",
    "rag-seed:tier1": "node scripts/rag-seed.js --tier 1",
    "rag-seed:tier2": "node scripts/rag-seed.js --tier 2",
    "rag-seed:tier3": "node scripts/rag-seed.js --tier 3",
    "test:e2e": "npx playwright test",
    "test:e2e:phase1": "npx playwright test tests/phase1-basic-flow.spec.js",
    "test:e2e:phase2": "npx playwright test tests/phase2-batch-detail-edge.spec.js",
    "test:e2e:phase3": "npx playwright test tests/phase3-concurrent.spec.js",
    "test:e2e:phase4": "npx playwright test tests/phase4-data-integrity.spec.js",
    "test:e2e:phase5": "npx playwright test tests/phase5-stress.spec.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "clsx": "^2.1.1",
    "exceljs": "^4.4.0",
    "leaflet": "^1.9.4",
    "pdf-parse": "^2.4.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.13.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/vite": "^4.1.18",
    "@vitejs/plugin-react": "^4.3.0",
    "adm-zip": "^0.5.16",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "vite": "^6.0.0"
  }
}
